{
  "author": "Antonia Elsen",
  "name": "constellation-fm",
  "description": "A playlist visualization and organization app",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "env-cmd -f .env.dev env-cmd -f .env.local env-cmd -f .env.secret nodemon --experimental-specifier-resolution=node --experimental-modules dist/index.js",
    "watch:compile": "tsc -w --preserveWatchOutput",
    "watch": "tsc && concurrently \"npm run watch:compile\" \"npm run start\""
  },
  "dependencies": {
    "axios": "^0.21.1",
    "cookie-parser": "^1.4.5",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-openid-connect": "^2.2.1",
    "express-session": "^1.17.2",
    "express-winston": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "passport": "^0.4.1",
    "passport-apple-music": "^1.0.1",
    "passport-auth0": "^1.4.0",
    "passport-spotify": "^2.0.0",
    "querystring": "~0.2.0",
    "spotify-web-api-node": "^4.0.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/passport": "^1.0.7",
    "concurrently": "^6.2.1",
    "env-cmd": "^10.1.0",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  }
}
